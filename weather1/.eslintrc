{
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "browser": true,
      "jquery": true
    },
    "rules": {
      "semi": 1,
      "indent": ["warn", 2],
      # "no-console": "warn",
      # "no-debugger": "warn"
    },
    # added line of code below to pass values from .env to bundle so that "process" does not appear as an undefined var
    "globals": {
        "process": true
      }
}
